# Config file for ETHOS runs
#here some parameters changed wrt EOS 21 to calculate difference

[run]
name : EoS_mini
output_dir : /Users/julian/Dropbox/Research/21/21cmFAST_ETHOS/21cmfish-master/docs/source/tutorials/DiffTest/

# redshifts
[redshifts]
min : 5.0
max : 35.0

# UserParam
[user_params]
HII_DIM : 200
BOX_LEN : 400
USE_FFTW_WISDOM : True
USE_INTERPOLATION_TABLES : True
FAST_FCOLL_TABLES : True
USE_RELATIVE_VELOCITIES : True
POWER_SPECTRUM : 5

# AstroParam
# gives best match to Planck tau for smooth-k window function
# changed F_ESC10 ad F_STAR10 (0.1 up and down), F_STAR7_MINI (0.25 up), and NU_X_THRESH (50 eV up)
[astro_params]
ALPHA_ESC: -0.3
F_ESC10 : -1.25
ALPHA_STAR : 0.5
F_STAR10 : -1.35
t_STAR : 0.5
F_STAR7_MINI : -2.25
ALPHA_STAR_MINI : 0.0
F_ESC7_MINI : -1.35
L_X : 40.5
L_X_MINI : 40.5
NU_X_THRESH : 550.0
A_VCB : 1.0
A_LW : 2.0

# FlagOptions
[flag_options]
INHOMO_RECO : True
USE_MASS_DEPENDENT_ZETA : True
USE_TS_FLUCT : True
USE_MINI_HALOS : True
FIX_VCB_AVG : False
USE_ETHOS : False
FILTER : 0

# Parameters to vary in the Fisher analysis
[vary]
astro_params_vary :
